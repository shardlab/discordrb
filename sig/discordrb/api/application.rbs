# API calls for slash commands.
module Discordrb::API::Application
  # Get a list of global application commands.
  # https://discord.com/developers/docs/interactions/slash-commands#get-global-application-commands
  def self?.get_global_commands: (untyped token, untyped application_id) -> untyped

  # Get a global application command by ID.
  # https://discord.com/developers/docs/interactions/slash-commands#get-global-application-command
  def self?.get_global_command: (untyped token, untyped application_id, untyped command_id) -> untyped

  # Create a global application command.
  # https://discord.com/developers/docs/interactions/slash-commands#create-global-application-command
  def self?.create_global_command: (untyped token, untyped application_id, untyped name, untyped description, ?untyped options, ?untyped? default_permission, ?::Integer `type`) -> untyped

  # Edit a global application command.
  # https://discord.com/developers/docs/interactions/slash-commands#edit-global-application-command
  def self?.edit_global_command: (untyped token, untyped application_id, untyped command_id, ?untyped? name, ?untyped? description, ?untyped? options, ?untyped? default_permission, ?::Integer `type`) -> untyped

  # Delete a global application command.
  # https://discord.com/developers/docs/interactions/slash-commands#delete-global-application-command
  def self?.delete_global_command: (untyped token, untyped application_id, untyped command_id) -> untyped

